{
  "name": "itl-quiz-game",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "start": "concurrently \"yarn start:client\" \"yarn start:server\"",
    "start:client": "yarn --cwd client start",
    "start:server": "yarn --cwd server dev",
    "build": "yarn --cwd client build"
  },
  "devDependencies": {
    "concurrently": "^6.2.1"
  }
}
